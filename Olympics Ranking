#Problem Code: OLYRANK


#include<iostream>
using namespace std;
int main()
{
	ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	int t;
	cin>>t;
	while(t--)
	{
		int a=0;
		int b=0;
		for(int i=0;i<3;i++)
		{
			int temp;
			cin>>temp;
			a+=temp;
		}
		for(int j=0;j<3;j++)
        {
        	int temp;
        	cin>>temp;
        	b+=temp;
		}
		if(a>b)
		cout<<"1";
		else
		cout<<"2";
		cout<<endl;
		
	}
	return 0;
	
}
