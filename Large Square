#Problem Code: XLSQUARE

#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int main()
{
	ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	int t;
	cin>>t;
	while(t--)
	{
		 int n,a;
		 cin>>n>>a;
		 int ways = (int)(sqrt(n));
		 int res= ways*a;
		 cout<<res<<endl;
} 
		
	return 0;
	
}
